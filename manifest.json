{
  "name": "Instabot",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Use Instabot to like/comment/follow with specified conditions!",
  "homepage_url": "http://github.com/chris542",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
      "default_icon":"icon.png",
      "default_title": "Instabot"
  },
  "permissions": [
    "https://www.instagram.com/*",
    "tabs"
  ]
}
